{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "indent": 0,
    "quotes": [
      "error",
      "single"
    ],
    "comma-dangle": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
